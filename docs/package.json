{
  "name": "docs-docker-mini-network",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "build:github": "node scripts/fix-yaml.mjs && GITHUB_PAGES=true next build",
    "build:local": "node scripts/fix-yaml.mjs && next build",
    "dev": "next dev --turbo",
    "start": "next start",
    "postinstall": "fumadocs-mdx",
    "fix-yaml": "node scripts/fix-yaml.mjs",
    "restore-yaml": "find content -name '*.backup' -exec sh -c 'mv \"$1\" \"${1%.backup}\"' _ {} \\;"
  },
  "dependencies": {
    "fumadocs-core": "15.7.1",
    "fumadocs-mdx": "11.8.0",
    "fumadocs-ui": "15.7.1",
    "js-yaml": "^4.1.0",
    "mermaid": "^11.10.1",
    "next": "15.5.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.12",
    "@types/mdx": "^2.0.13",
    "@types/node": "24.3.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "eslint": "^8",
    "eslint-config-next": "15.5.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.12",
    "typescript": "^5.9.2"
  }
}
